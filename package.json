{
  "name": "create-pubsub",
  "version": "1.6.0",
  "description": "A tiny Event Emitter and Observable Store.",
  "files": [
    "main",
    "react",
    "immer"
  ],
  "types": "./main/index.d.ts",
  "main": "./main/index.js",
  "module": "./main/index.module.js",
  "exports": {
    ".": {
      "import": "./main/index.module.js",
      "require": "./main/index.js"
    },
    "./react": {
      "import": "./react/index.module.js",
      "require": "./react/index.js"
    },
    "./immer": {
      "import": "./immer/index.module.js",
      "require": "./immer/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "del main react immer && rollup -c --configPlugin typescript",
    "test": "uvu -r ts-node/register -r global-jsdom/register test",
    "test-watch": "watchlist test -- npm test",
    "prepare": "package-check && npm run build",
    "preview-package-contents": "npm pack --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felladrin/create-pubsub.git"
  },
  "keywords": [
    "event",
    "events",
    "dispatcher",
    "emitter",
    "listener",
    "listeners",
    "event-emitter",
    "subscribe",
    "state",
    "state-management"
  ],
  "author": {
    "name": "Victor Nogueira",
    "email": "victor@victornogueira.app",
    "url": "https://victornogueira.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felladrin/create-pubsub/issues"
  },
  "homepage": "https://github.com/felladrin/create-pubsub#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "11.0.0",
    "@skypack/package-check": "0.2.2",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.0.28",
    "del-cli": "5.0.0",
    "global-jsdom": "8.7.0",
    "immer": "9.0.19",
    "jsdom": "21.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "3.19.1",
    "rollup-plugin-dts": "5.2.0",
    "rollup-plugin-terser": "7.0.2",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "uvu": "0.5.6",
    "watchlist": "0.3.1"
  }
}
