{
  "name": "create-pubsub",
  "version": "1.3.1",
  "description": "A tiny (166 bytes) strongly-typed Emitter/Listener which is also a Store.",
  "files": [
    "dist"
  ],
  "source": "./src/create-pubsub.ts",
  "types": "./dist/create-pubsub.d.ts",
  "exports": {
    "browser": "./dist/create-pubsub.module.js",
    "umd": "./dist/create-pubsub.umd.js",
    "import": "./dist/create-pubsub.modern.js",
    "require": "./dist/create-pubsub.js",
    "default": "./dist/create-pubsub.module.js"
  },
  "main": "./dist/create-pubsub.js",
  "module": "./dist/create-pubsub.module.js",
  "unpkg": "./dist/create-pubsub.umd.js",
  "sideEffects": false,
  "scripts": {
    "build": "microbundle src/create-pubsub.ts --name create-pubsub --sourcemap false",
    "dev": "microbundle watch src/create-pubsub.ts --name create-pubsub --no-compress",
    "test": "uvu -r ts-node/register test",
    "prepare": "package-check && del dist && npm run build",
    "preview-package-contents": "npm pack --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felladrin/create-pubsub.git"
  },
  "keywords": [
    "event",
    "events",
    "dispatcher",
    "emitter",
    "listener",
    "listeners",
    "event-emitter",
    "subscribe",
    "state",
    "state-management",
    "pubsub",
    "pub-sub",
    "pub/sub",
    "publish/subscribe",
    "publish-subscribe"
  ],
  "author": {
    "name": "Victor Nogueira",
    "email": "victor@victornogueira.app",
    "url": "https://victornogueira.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felladrin/create-pubsub/issues"
  },
  "homepage": "https://github.com/felladrin/create-pubsub#readme",
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "del-cli": "4.0.1",
    "microbundle": "0.14.2",
    "ts-node": "10.4.0",
    "uvu": "0.5.2"
  }
}
