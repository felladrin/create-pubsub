{
  "name": "create-pubsub",
  "version": "1.4.0",
  "description": "A tiny Event Emitter and Observable Store.",
  "files": [
    "main",
    "react",
    "derived"
  ],
  "types": "./main/index.d.ts",
  "main": "./main/index.js",
  "module": "./main/index.module.js",
  "exports": {
    ".": {
      "import": "./main/index.module.js",
      "require": "./main/index.js"
    },
    "./react": {
      "import": "./react/index.module.js",
      "require": "./react/index.js"
    },
    "./derived": {
      "import": "./derived/index.module.js",
      "require": "./derived/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "del main react derived && rollup -c",
    "test": "uvu -r ts-node/register test",
    "prepare": "package-check && npm run build",
    "preview-package-contents": "npm pack --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felladrin/create-pubsub.git"
  },
  "keywords": [
    "event",
    "events",
    "dispatcher",
    "emitter",
    "listener",
    "listeners",
    "event-emitter",
    "subscribe",
    "state",
    "state-management"
  ],
  "author": {
    "name": "Victor Nogueira",
    "email": "victor@victornogueira.app",
    "url": "https://victornogueira.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felladrin/create-pubsub/issues"
  },
  "homepage": "https://github.com/felladrin/create-pubsub#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "8.3.1",
    "@skypack/package-check": "0.2.2",
    "@types/react": "18.0.2",
    "del-cli": "4.0.1",
    "rollup": "2.70.1",
    "rollup-plugin-dts": "4.2.1",
    "rollup-plugin-terser": "7.0.2",
    "ts-node": "10.7.0",
    "tslib": "2.3.1",
    "typescript": "4.6.3",
    "uvu": "0.5.3"
  }
}
